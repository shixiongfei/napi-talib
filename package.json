{
  "name": "napi-talib",
  "version": "0.1.0",
  "description": "Node API for TA-Lib",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "src/*.cpp",
    "src/ta-lib/include/",
    "src/ta-lib/src/",
    "index.js",
    "index.d.ts",
    "binding.gyp"
  ],
  "scripts": {
    "build": "node-gyp clean && node-gyp-build",
    "build:debug": "node-gyp clean && node-gyp configure && node-gyp build --debug",
    "install": "node-gyp-build",
    "prebuild": "prebuildify --napi",
    "test": "node test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shixiongfei/napi-talib.git"
  },
  "keywords": [
    "ta-lib",
    "talib",
    "trading",
    "indicators",
    "technical analysis"
  ],
  "author": "shixiongfei",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/shixiongfei/napi-talib/issues"
  },
  "homepage": "https://github.com/shixiongfei/napi-talib#readme",
  "dependencies": {
    "node-gyp-build": "^4.8.1"
  },
  "devDependencies": {
    "node-gyp": "^10.1.0",
    "prebuildify": "^6.0.1"
  }
}
